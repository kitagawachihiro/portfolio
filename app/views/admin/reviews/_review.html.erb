<tr>
  <td>
    <%= review.id %>
  </td>
  <td>
    <%= review.todo_id %>
  </td>
  <td>
    <%= review.rating %>
  </td>
  <td>
    <%= review.comment %>
  </td>
  <td>
    <%= l review.created_at, formats: :long %>
  </td>
  <td>
    <%= l review.updated_at, formats: :long %>
  </td>
  <td>
    <%= link_to t('defaults.delete'), admin_review_path(review), method: :delete, data: { confirm: '削除しますか？' }, class: 'btn btn-danger' %>
  </td>
</tr>
